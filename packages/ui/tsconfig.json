{
    "compilerOptions": {
      "target": "ES2017",
      "lib": ["DOM", "DOM.Iterable", "ES2017"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "ESNext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react",
      "esModuleInterop": true
    },
    "include": ["src"]
  }
